package br.com.fiap.tds.entity;

import java.util.Calendar;

public class Aluno {
	private int codigo;
	private String nome;
	private Calendar dataNascimento;
	private Genero genero;
	private int codigoGrupo;

	public Aluno(int codigo, String nome, Calendar dataNascimento, Genero genero, int codigoGrupo) {
		super();
		this.codigo = codigo;
		this.nome = nome;
		this.dataNascimento = dataNascimento;
		this.genero = genero;
		this.codigoGrupo = codigoGrupo;
	}

	public Aluno(String nome, Calendar dataNascimento, Genero genero, int codigoGrupo) {
		super();
		this.nome = nome;
		this.dataNascimento = dataNascimento;
		this.genero = genero;
		this.codigoGrupo = codigoGrupo;
	}

	public Aluno() {
	}

}
