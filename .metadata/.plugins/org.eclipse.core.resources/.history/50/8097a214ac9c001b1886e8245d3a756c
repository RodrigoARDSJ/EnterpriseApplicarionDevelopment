package br.com.fiap.tds.entity;

public class GrupoChallenge {
	private int codigo;
	private String nome;

	public GrupoChallenge(int codigo, String nome) {
		super();
		this.codigo = codigo;
		this.nome = nome;
	}

	public GrupoChallenge(String nome) {
		super();
		this.nome = nome;
	}

	public GrupoChallenge() {
	}

}
